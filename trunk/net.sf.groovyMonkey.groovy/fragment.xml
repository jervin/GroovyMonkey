<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.2"?>
<fragment>
   <extension
         point="net.sf.groovyMonkey.lang">
         <lang
            factory="net.sf.groovyMonkey.groovy.GroovyMonkeyScriptFactory"
            name="Groovy"/>
   </extension>

	<extension
         point="net.sf.groovyMonkey.dom">
      <updateSite url="http://groovy-monkey.sourceforge.net/update/plugins/net.sf.groovyMonkey.groovy"/>
      <dom
            class="groovy.jface.JFaceDOMFactory"
            variableName="jface"/>
   </extension>

</fragment>
